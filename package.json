{
  "name": "react-walkthru",
  "version": "0.0.2",
  "description": "Easy to use step-by-step site guide component for React.",
  "main": "lib/index.js",
  "scripts": {
    "start-dev": "babel ./src --watch --out-dir ./lib & cd demo && npm install && npm run start-dev",
    "prebuild": "rm -rf lib/*",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "walk",
    "through",
    "walkthrough",
    "guide",
    "react"
  ],
  "author": "Burak Can <neoberg@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/Adphorus/react-walkthru"
  },
  "bugs": {
    "url": "http://github.com/Adphorus/react-walkthru/issues"
  },
  "dependencies": {
    "react": ">=0.13.0"
  },
  "devDependencies": {
    "babel": "^5.8.34"
  }
}
